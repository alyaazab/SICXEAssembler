myprog   start   1000
lbl      word    5
tst      byte    x'ab'
bgn      lda     #3
         ldx     #5
         lda     lbl
         tixr    s
         rmo     a,x
         org     tst
         lda     #tst
         ldt     #3
         ldx     #8
         ldx     #5
         lda     lbl
         tixr    a
         rmo     s,x
         END     bgn